{
	"name": "l4l-citizen-e2e",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"projectType": "application",
	"sourceRoot": "apps/l4l-citizen-e2e/src",
	"tags": [],
	"implicitDependencies": ["l4l-citizen"],
	"targets": {
		"e2e": {
			"executor": "@nx/cypress:cypress",
			"options": {
				"cypressConfig": "apps/l4l-citizen-e2e/cypress.json",
				"testingType": "e2e",
				"devServerTarget": "l4l-citizen:serve:development"
			},
			"configurations": {
				"production": {
					"devServerTarget": "l4l-citizen:serve:production"
				},
				"ci": {
					"devServerTarget": "l4l-citizen:serve-static"
				}
			}
		},
		"lint": {
			"executor": "@nx/eslint:lint"
		}
	}
}
