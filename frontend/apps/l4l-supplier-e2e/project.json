{
	"name": "l4l-supplier-e2e",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "apps/l4l-supplier-e2e/src",
	"projectType": "application",
	"tags": [],
	"implicitDependencies": ["l4l-supplier"],
	"targets": {
		"e2e": {
			"executor": "@nx/cypress:cypress",
			"options": {
				"cypressConfig": "apps/l4l-supplier-e2e/cypress.config.ts",
				"devServerTarget": "l4l-supplier:serve:development",
				"testingType": "e2e"
			},
			"configurations": {
				"production": {
					"devServerTarget": "l4l-supplier:serve:production"
				},
				"ci": {
					"devServerTarget": "l4l-supplier:serve-static"
				}
			}
		},
		"lint": {
			"executor": "@nx/eslint:lint",
			"outputs": ["{options.outputFile}"],
			"options": {
				"lintFilePatterns": ["apps/l4l-supplier-e2e/**/*.{js,ts}"]
			}
		}
	}
}
